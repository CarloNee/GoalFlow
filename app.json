{
  "expo": {
    "name": "Goalflow",
    "slug": "Goalflow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carlodev.Goalflow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.carlodev.Goalflow"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "react-native-add-calendar-event",
        {
          "calendarPermission": "Our great calendar app wants to access your calendar",
          "contactsPermission": "Our great calendar app wants to access your contacts"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b1628667-74a5-4735-8244-b1f31dd56016"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b1628667-74a5-4735-8244-b1f31dd56016"
    }
  }
}
